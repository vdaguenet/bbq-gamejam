.button-start
  text-transform uppercase
  cursor pointer
  transition opacity 0.6s
  display inline-block
  padding 12px 35px
  border 1px solid #ba864c
  border-radius 6px
  color #ba864c
  background #e6c16d
  position absolute
  left 50%
  bottom 70px
  transform translateX(-50%)

  &:hover
    opacity 0.7
